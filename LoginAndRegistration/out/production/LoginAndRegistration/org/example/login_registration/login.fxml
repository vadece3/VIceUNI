<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="386.0" prefWidth="474.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.login_registration.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <AnchorPane prefHeight="342.0" prefWidth="434.0">
        <Label fx:id="status" layoutX="14.0" layoutY="7.0" prefHeight="61.0" prefWidth="419.0" />
        <Button layoutX="194.0" layoutY="209.0" onAction="#login" text="Login" />
        <TextField fx:id="user" layoutX="149.0" layoutY="110.0" promptText="-------Enter user-------" />
        <TextField fx:id="password" layoutX="149.0" layoutY="155.0" promptText="-------Enter password-------" />
        <Button layoutX="179.0" layoutY="262.0" mnemonicParsing="false" onAction="#register" prefHeight="26.0" prefWidth="75.0" text="Register" />
    </AnchorPane>
</VBox>
